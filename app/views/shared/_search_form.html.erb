<%= search_form_for @q, url: search_path, class: 'form-inline', id: "search-article-form" do |f| %>
    <%= f.search_field :title_or_content_cont, placeholder: 'キーワードを入力', class: 'search-field search-input' %>
    <%= button_tag type: 'submit', class: 'search-box' do %>
        <i class="fas fa-search"></i>
    <% end %>
<% end %>
<%= javascript_include_tag "navbar.js" %>